<Application x:Class="ms.video.downloader.win.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:local="clr-namespace:ms.video.downloader"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:DatabindingEntryConverter x:Key="EntryConverter" />
            <local:DatabindingPauseButtonContentConverter x:Key="PauseContentConverter" />
            <local:DatabindingProgressBarColorConverter x:Key="ProgressBarColorConverter" />
            <local:BooleanToVisibilityConverter x:Key="VisibilityConverter"/>
            <local:OppositeBooleanToVisibilityConverter x:Key="OppositeVisibilityConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
